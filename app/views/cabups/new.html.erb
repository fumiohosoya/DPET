<h3>Cabup Check</h3>
<div class="row">
    <div class="offset-1 col-10">
        <div class="row">
            <div class="col-6">
                <%= image_tag '/images/GuidanceCabup1.png', alt: 'Guidancecabup1', class: 'img-fluid' %>
            </div>
            <div class="col-6">
                <%= image_tag '/images/GuidanceCabup2.png', alt: 'Guidancecabup2', class: 'img-fluid' %>
            </div>
        </div>
    </div>
</div>
   <%= form_with(model: @cabup, local:true, multipart: true) do |f|  %>
    
  <%= f.hidden_field :driver_id, value: @current_driver.id %>
    
      <%= image_tag '/images/GuidanceCabup3.png', alt: 'Guidancecabup3', class: 'img-fluid' %>
      <%= f.fields_for :checkimages do |q| %>
            <% if f.object.checkimages.first&.image.present? %>
              <%= image_tag f.object.checkimages.first.image.url(:thumb) %>
              <%= q.hidden_field :image_cache %>
            <% else %>
                <%= q.file_field :image, accept:'image/*', capture:'environment' %>
            <% end %>
      <% end %>
      <%= f.fields_for :checkimages do |q| %>
            <% if f.object.checkimages.second&.image.present? %>
              <%= image_tag f.object.checkimages.second.image.url(:thumb) %>
              <%= q.hidden_field :image_cache %>
            <% else %>
                <%= q.file_field :image, accept:'image/*', capture:'environment' %>
            <% end %>
      <% end %>
      <%= f.fields_for :checkimages do |q| %>
            <% if f.object.checkimages.third&.image.present? %>
              <%= image_tag f.object.checkimages.third.image.url(:thumb) %>
              <%= q.hidden_field :image_cache %>
            <% else %>
                <%= q.file_field :image, accept:'image/*', capture:'environment' %>
            <% end %>
      <% end %>
      
      <div class="form-group row">
          <%= f.submit "Send Photo", class:"btn btn-xs btn-danger" %>
      </div>
    <% end %>