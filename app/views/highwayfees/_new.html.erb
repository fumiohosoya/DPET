
<h2>Highway fee</h2>
<%= form_with(model: Highwayfee.new(vehicle_id: @vehicle_id), remote:true) do |f| %>
   <div class="form-group">
       <%= f.label :date, "Date" %>
       <%= f.date_field :date, class:"form-control" %>
   </div>
   <div class="form-group">
       <%= f.label :price, "Total_Amount" %>
       <%= f.text_field :price, class:"form-control" %>
   
　 <div class="form-group">
　  <%= f.hidden_field :vehicle_id, value: @vehicle_id %>

    <%= f.submit "Regist" %>
   </div>  

<% end %>