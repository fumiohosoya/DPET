<h3>AirReserver Check</h3>
<div class="row">
    <div class="offset-2 col-4">
        <%= image_tag '/images/GuidanceAirReserver1.png', alt: 'GuidanceAirReserver1', class: 'img-fluid' %>
        <%#= image_tag 'images/GuidanceMeter1.png', class="img-fluid" %>
    </div>
</div>
   <%= form_with(model: @airreserver, local:true, multipart: true) do |f|  %>
    
    
      <%= f.hidden_field :driver_id, value: @current_driver.id %>
      <%= f.fields_for :checkimages do |q| %>
            <% if q.object.image.present? %>
              <%= image_tag q.object.image.url(:thumb) %>
              <%= q.hidden_field :image_cache %>
            <% else %>
                <%= image_tag '/images/GuidanceBrake1.png', alt: 'GuidanceMeter1', class: 'img-fluid' %>
                <%= q.file_field :image, accept:'image/*', capture:'environment' %>
            <% end %>

          
          
            <!--<input type="file" name="image" accept="video/*" capture="environment">-->

          
          
      <% end %>
      
      <div class="form-group row">
          <%= f.submit "Send Photo", class:"btn btn-xs btn-danger" %>
      </div>
    <% end %>