<%= render 'admin/backtoadmin' %>
<h2><%=  find_company(@company_id).name_e %></h2> 
<div class="bgwhite row mt-4">
    

    
    
    <% if @result.any? %>
    
        <table class="table table-striped table-bordered bgwhite">
            <tr>
               <th>Name</th>  <th>Operation</th> <th>Empty </th>
               <th>Occupied </th> <th>Mileage </th> <th>Handling </th>
               <th>SpeedOver </th> <th>SpOver Time</th> <th>Scramble</th>
               <th>Rapid Accel</th> <th>Abrupt Decel</th> <th>Idling</th>
               <th>Running</th> <th>Evaluate</th> <th>Rank</th>
            </tr>
            <% @result.each do |e| %>
                <tr>
                    <td><%= Driver.find(e.driver_id).name %></td>
                    <td><%= e.op_count %></td>
                    <td><%= e.empty_conv %></td>
                    <td><%= e.occupied_conv %></td>
                    <td><%= e.mileage %></td>
                    <td><%= timecount_hm(e.handling) %></td>
                    <td><%= e.speedover%></td>
                    <td><%= timecount_hm(e.spover_time) %></td>
                    <td><%= e.scramble %></td>
                    <td><%= e.rapid_accel%></td>
                    <td><%= e.abrupt_decel%></td>
                    <td><%= timecount_hm(e.idling) %></td>
                    <td><%= timecount_hm(e.running) %></td>
                    <td><%= e.evaluate %></td>
                    <td><%= e.rank %></td>
                </tr>
            <% end %>
            
        </table>
    <% end %>
    
</div>